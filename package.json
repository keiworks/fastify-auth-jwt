{
  "name": "@keiworks/fastify-auth-jwt",
  "type": "module",
  "version": "0.1.0",
  "author": "keiworks",
  "license": "MIT",
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keiworks/fastify-auth-jwt.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "@keiworks:registry": "https://npm.pkg.github.com"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "format": "prettier --loglevel error --write ./src ./tests",
    "lint": "eslint --fix ./src ./tests",
    "test": "vitest",
    "build": "pnpm run format && pnpm run lint && vitest run && tsc -p ./tsconfig.build.json",
    "prepare": "husky install & prisma generate",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "fastify-plugin": "4.5.0",
    "ms": "2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@prisma/client": "4.15.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/ms": "0.7.31",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "vitest": "0.31.3"
  },
  "peerDependencies": {
    "@keiworks/js-utilities": "^0.1.0",
    "argon2": "^0.30.3",
    "fastify": "^4.17.0",
    "jsonwebtoken": "^9.0.0",
    "zod": "^3.21.4"
  }
}
